{
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "breadcrumbs.enabled": true,
    "codestats.apikey": "SFMyNTY.UVhacGIzSjBhR1ZyYVc1biMjTmpVNQ.q3xB0NT-WfD1Wa10NKpvEuRI9RE3kICfOgj3JsyFAPM",
    "codestats.username": "Aviortheking",
    "debug.console.fontFamily": "'Fira Code'",
    "debug.console.fontSize": 18,
    "debug.inlineValues": true,
    "debug.internalConsoleOptions": "neverOpen",
    "debug.openDebug": "neverOpen",
    "debug.toolBarLocation": "floating",
    "discord.detailsDebugging": "Debugging {workspace}",
    "discord.detailsIdle": "{null}",
    "discord.largeImage": "Editing a {lang} file",
    "discord.largeImageIdle": "zZzZz",
    "discord.lowerDetailsEditing": "Line: {currentline} on {totallines}",
    "discord.lowerDetailsIdle": "{null}",
    "discord.lowerDetailsNotFound": "Line: {currentline} on {totallines}",
    "discord.workspaceElapsedTime": true,
    "editor.cursorBlinking": "phase",
	"editor.cursorStyle": "block",
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.fontWeight": "bold",
    "editor.lineNumbers": "interval",
    "editor.parameterHints.cycle": true,
    "editor.renderWhitespace": "boundary",
    "editor.suggest.localityBonus": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "emmet.includeLanguages": {
        "html.twig": "html",
        "njk": "html",
        "svg": "html",
        "twig": "html",
        "xml.twig": "xml"
    },
    "java.home": "/usr/lib/jvm/java-8-openjdk/",
    "explorer.sortOrder": "type",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.associations": {
        "*.html.twig": "twig",
        "*.lock": "json",
        "*.njk": [
            "twig",
            "html"
        ],
        "*.svg": "html",
        "*.xml.twig": [
            "xml",
            "html",
            "twig"
        ],
        "yarn.lock": "yaml"
    },
    "files.autoSave": "off",
    "files.watcherExclude": {
        "**/bin/**": true,
        "**/config/**": true,
        "**/public/**": true,
        "**/templates/**": true,
        "**/tests/**": true,
        "**/translations/**": true,
        "**/var/**": true,
        "**/vendor/**": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "gitlens.blame.toggleMode": "window",
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "details": false,
            "history": false,
            "remote": false
        },
        "editorGroup": {
            "blame": true,
            "compare": true
        },
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true,
            "stashInline": true
        }
    },
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.recentChanges.toggleMode": "window",
    "gitlens.statusBar.alignment": "left",
    "gitlens.views.compare.enabled": false,
    "gitlens.views.compare.location": "scm",
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.views.repositories.files.layout": "list",
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.search.enabled": false,
    "gitlens.views.search.location": "scm",
    "grunt.autoDetect": "off",
    "gulp.autoDetect": "off",
    "java.completion.guessMethodArguments": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.dependency.packagePresentation": "hierarchical",
    "java.implementationsCodeLens.enabled": true,
    "java.referencesCodeLens.enabled": true,
    "java.saveActions.organizeImports": true,
    "java.signatureHelp.enabled": true,
    "java.trace.server": "messages",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "npm.enableScriptExplorer": true,
    "npm.exclude": [
        "**/vendor/**"
    ],
    "npm.packageManager": "yarn",
    "php-docblocker.author": {
        "email": "florian.bouillon@delta-wings.net",
        "name": "Avior"
    },
    "php-docblocker.returnGap": true,
    "php.executablePath": "/usr/bin/php",
    "php.validate.executablePath": "/usr/bin/php",
    "php.validate.run": "onType",
    "phpmd.validate.rulesets": "codesize,controversial,design,naming,unusedcode",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "showGitMetrics": true,
    "showMusicMetrics": true,
    "showWeeklyRanking": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.enableBell": true,
    "terminal.integrated.fontFamily": "'Fira Code'",
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.fontWeight": "bold",
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.coffee",
        "**/*.pug"
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.closeWhenEmpty": true,
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "New Moon",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.sideBar.location": "left",
    "workbench.statusBar.feedback.visible": false,
    "workbench.tree.indent": 20,
    "workbench.useExperimentalGridLayout": true,
    "workbench.view.alwaysShowHeaderActions": true,
    "yaml.format.enable": true,
    "yaml.format.singleQuote": true,
    "zenMode.centerLayout": false,
	"zenMode.hideTabs": false,
	"terminal.integrated.rendererType": "dom",
    "workbench.statusBar.visible": true,
    "emojisense.languages": {
        "markdown": true,
        "plaintext": {
          "markupCompletionsEnabled": false,
          "emojiDecoratorsEnabled": false
        },
        "git-commit": true,
        "jade": true,
        "go": true
    },
    "editor.renderControlCharacters": false,
    "diffEditor.renderSideBySide": true,
    "editor.detectIndentation": false,
    "files.trimFinalNewlines": true,
    "vscode-mysql.enableTelemetry": false,
    "eslint.packageManager": "yarn",
    "eslint.provideLintTask": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "maven.terminal.useJavaHome": true
}
